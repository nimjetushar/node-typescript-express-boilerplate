{
  "name": "node-typescript-express-boilerplate",
  "version": "0.3.0",
  "description": "boilerplate for node-express with typescript",
  "main": "dist",
  "scripts": {
    "start": "node dist",
    "prestart": "npm run -s build-ts",
    "dev": "concurrently -k -p \"[{name}]\" -n \"TypeScript,Node\" -c \"cyan.bold,green.bold\" \"npm run build-ts:watch\" \"npm run node-watch\"",
    "debug": "concurrently -k -p \"[{name}]\" -n \"TypeScript,Node\" -c \"cyan.bold,green.bold\" \"npm run build-ts:watch\" \"npm run node-debug\"",
    "build-ts": "tsc -d true",
    "build-ts:watch": "tsc -w -d true",
    "node-watch": "nodemon dist",
    "node-debug": "nodemon --inspect dist",
    "test": "jest"
  },
  "engines": {
    "node": ">=8.0.0",
    "npm": ">=5.5.0"
  },
  "author": "Tushar Nimje",
  "license": "MIT",
  "dependencies": {
    "body-parser": "1.19.0",
    "compression": "1.7.4",
    "express": "4.16.4",
    "cors": "2.8.5",
    "helmet": "3.18.0",
    "mongoose": "5.6.2",
    "morgan": "1.9.1"
  },
  "devDependencies": {
    "@babel/cli": "7.0.0",
    "@babel/core": "7.4.5",
    "@babel/plugin-proposal-class-properties": "7.4.0",
    "@babel/plugin-proposal-object-rest-spread": "7.4.4",
    "@babel/preset-env": "7.4.5",
    "@babel/preset-typescript": "7.3.3",
    "@types/compression": "1.7.0",
    "@types/cors": "2.8.5",
    "@types/jest": "24.0.13",
    "@types/node": "11.13.8",
    "@types/supertest": "2.0.7",
    "jest": "24.5.0",
    "supertest": "4.0.2",
    "ts-jest": "24.0.2",
    "@types/express": "4.16.1",
    "@types/helmet": "0.0.43",
    "@types/mongoose": "5.2.13",
    "@types/morgan": "1.7.35",
    "concurrently": "4.1.0",
    "nodemon": "1.19.1",
    "rimraf": "2.6.3",
    "typescript": "3.5.2"
  }
}
